{
  "name": "nginx-nextjs-setup",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "prisma:migrate": "pnpm prisma migrate dev --schema=lib/prisma/schema.prisma",
    "prisma:deploy": "pnpm prisma migrate deploy --schema=lib/prisma/schema.prisma && pnpm run prisma:generate",
    "prisma:generate": "pnpm prisma generate --schema=lib/prisma/schema.prisma",
    "prisma:seed": "pnpm tsx lib/prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "6.9.0",
    "dotenv": "^17.2.3",
    "mysql2": "^3.16.3",
    "next": "16.1.6",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "prisma": "6.9.0",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}
